{
  "name": "@datalackey/gas-demodulify",
  "version": "0.1.0",
  "description": "Webpack plugin that emits GAS-safe JavaScript with hierarchical namespaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc --watch",
    "pretest": "npm run compile",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rm -rf dist && npm run compile && npm run test",
    "docs:toc:update": "update-markdown-toc --recursive .",
    "docs:toc:check": "update-markdown-toc --check --recursive .",
    "package:release": "npm run docs:toc:check &&  npm run build && node scripts/build-plugin-package.js"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "devDependencies": {
    "@datalackey/update-markdown-toc": "^1.0.1",
    "@types/jest": "^30.0.0",
    "jest": "^29.7.0",
    "outdent": "^0.8.0",
    "strip-comments": "^2.0.1",
    "ts-dedent": "^2.2.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "zod": "^4.3.5"
  }
}
