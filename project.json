{
    "name": "gas-demodulify",
    "sourceRoot": "src",
    "projectType": "library",
    "targets": {
        "lint": {
            "command": "npm run lint"
        },
        "format:check": {
            "command": "npm run format:check"
        },
        "toc:check": {
            "command": "npm run docs:toc:check"
        },
        "compile": {
            "command": "tsc -p tsconfig.build.json",
            "outputs": ["{projectRoot}/dist"]
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{projectRoot}/coverage"],
            "options": {
                "jestConfig": "{projectRoot}/jest.config.cjs",
                "passWithNoTests": true
            }
        },
        "package": {
            "dependsOn": ["compile"],
            "command": "node scripts/build-plugin-package.js"
        },
        "build": {
            "dependsOn": ["lint", "format:check", "toc:check", "test"]
        },
        "release": {
            "dependsOn": ["build", "package"]
        }
    }
}
